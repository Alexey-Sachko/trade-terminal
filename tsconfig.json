{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "es6",
    "jsx": "react",

    "sourceMap": true,

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "preserveSymlinks": false,

    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  }
}
